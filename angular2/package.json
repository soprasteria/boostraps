{
  "name": "asm-ui",
  "version": "1.0.0",
  "scripts": {
    "init-project": "npm install && bower install --allow-root",
    "run-dev": "tsc && concurrently \"npm run tsc:w\" \"npm run server-dev\" ",
    "server-dev": "lite-server -c bs-config.js",
    "postinstall": "typings install",
    "tsc": "tsc",
    "run-prod": "http-server -p 3000 -s",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
